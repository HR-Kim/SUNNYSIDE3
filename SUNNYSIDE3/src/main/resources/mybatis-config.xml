		<typeAlias type="kr.co.sunnyside.movie.service.LHJ_BoxofficeVO" alias="Boxoffice" />
		<typeAlias type="kr.co.sunnyside.movie.service.LHJ_MovieVO" alias="Movie" />
		<typeAlias type="kr.co.sunnyside.code.service.CodeVO" alias="Code" />
		<typeAlias type="kr.co.sunnyside.cmn.SearchVO" alias="Search" />
		
		<!-- LGS -->
